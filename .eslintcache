th=\"/\" component={HomePage} />\n        <Route path=\"/shop\" component={ShopPage} />\n        <Route exact path=\"/checkout\" component={CheckoutPage} />\n        <Route\n          exact\n          path=\"/signin\"\n          render={() =>\n            currentUser ? <Redirect to=\"/\" /> : <SignInAndSignUpPage />\n          }\n        />\n      </Switch>\n    </div>\n  );\n}\n\nconst mapStateToProps = createStructuredSelector({\n  currentUser: selectCurrentUser,\n});\n\nconst mapDispatchToProps = (dispatch) => ({\n  setCurrentUser: (user) => dispatch(setCurrentUser(user)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(App);\n","/Users/nikhilgupta/Desktop/Github/niks-clothing/src/components/sign-up/sign-up.component.jsx",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/components/sign-in/sign-in.component.jsx",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/index.js",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/components/directory/directory.component.jsx",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/redux/user/user.reducer.js",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/components/header/header.component.jsx",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/redux/user/user.actions.js",[],["155","156"],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/components/cart-icon/cart-icon.component.jsx",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/components/cart-dropdown/cart-dropdown.component.jsx",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/redux/cart/cart.reducer.js",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/redux/cart/cart.actions.js",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/components/custom-button/custom-button.component.jsx",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/components/collection-item/collection-item.component.jsx",[],"/Users/nikhilgupta/Desktop/Github/niks-clothing/src/redux/cart/cart.types.js",[],"/Users/nikh